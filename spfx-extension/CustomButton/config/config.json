{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "custom-file-upload-command-set": {
      "components": [
        {
          "entrypoint": "./lib/extensions/customFileUpload/CustomFileUploadCommandSet.js",
          "manifest": "./src/extensions/customFileUpload/CustomFileUploadCommandSet.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "lodash": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/lodash.js/4.17.15/lodash.min.js",
      "globalName": "_"
    },
    "moment": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.26.0/moment-with-locales.min.js",
      "globalName": "moment"
    }
  },
  "localizedResources": {
    "CustomFileUploadCommandSetStrings": "lib/extensions/customFileUpload/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}
